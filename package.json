{
  "name": "telegram-github-stars",
  "version": "1.0.0",
  "description": "Telegram bot to notify subscribers about new stars on GitHub repositories",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "ts-node --watch src/index.ts",
    "test-polling": "ts-node scripts/test-polling.ts",
    "test-db": "ts-node scripts/test-database.ts",
    "test-webhook": "ts-node scripts/test-webhook.ts",
    "test-cron": "ts-node scripts/test-cron.ts",
    "troubleshoot": "ts-node scripts/troubleshoot.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "github",
    "stars",
    "notifications"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "@supabase/supabase-js": "^2.39.0",
    "body-parser": "^2.2.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.66.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.13",
    "@types/node-telegram-bot-api": "^0.64.7",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
